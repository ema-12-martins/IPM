: space-around;\n    align-items: center;\n    height: 39%; /* 13% por cada butao */\n}\n\n.side-container4{\n    margin-top: 2rem;\n    display: flex;\n    flex-direction: column;\n    justify-content: space-around;\n    align-items: center;\n    height: 52%; /* 13% por cada butao */\n}\n\n.side-buttonSelected {\n    border-radius: 1.3rem;\n    border: 0.3rem solid #000000;\n    background-color: #FFFFFF;\n    display: flex;\n    flex-direction: row;\n    justify-content: center;\n    padding: 1.25rem 0 1.25rem 0;\n    width: 80%;\n    font-family: 'Inter';\n    font-weight: normal;\n    font-size: 2rem;\n    color: #000000;\n    cursor: pointer;\n}\n\n.side-button {\n    border-radius: 1.3rem;\n    background-color: #FFFFFF;\n    display: flex;\n    flex-direction: row;\n    justify-content: center;\n    padding: 1.25rem 0 1.25rem 0;\n    width: 80%;\n    font-family: 'Inter';\n    font-weight: normal;\n    font-size: 2rem;\n    color: #000000;\n    cursor: pointer;\n}\n</style>"],"mappings":";AAuDA,OAAOA,eAAc,MAAO,uBAAuB;AACnD,eAAe;EACXC,IAAI,EAAE,YAAY;EAElBC,KAAK,EAAE;IACHC,iBAAiB,EAAEC;EACvB,CAAC;EAEDC,UAAU,EAAE;IACRL;EACJ,CAAC;EAEDM,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,YAAY,EAAE;IAClB;EACJ,CAAC;EAEDC,OAAO,EAAE;IACLC,kBAAkBA,CAACC,GAAG,EAAE;MACpB,IAAIC,QAAO,GAAI,IAAI,CAACC,MAAM,CAACC,IAAI,CAACC,KAAK,CAAC,GAAG;MACzCH,QAAQ,CAACA,QAAQ,CAACI,MAAM,GAAC,CAAC,IAAIL,GAAG;MACjC,IAAIM,IAAG,GAAIL,QAAQ,CAACM,IAAI,CAAC,GAAG;MAC5B,IAAI,CAACC,OAAO,CAACC,IAAI,CAACH,IAAI,CAAC;IAC3B,CAAC;IACDI,WAAWA,CAACJ,IAAI,EAAE;MACd,IAAI,CAACE,OAAO,CAACC,IAAI,CAACH,IAAI,CAAC;IAC3B,CAAC;IACDK,SAASA,CAACC,IAAI,EAAE;MACZ,IAAIX,QAAO,GAAI,IAAI,CAACC,MAAM,CAACC,IAAI,CAACC,KAAK,CAAC,GAAG;MACzC,OAAOH,QAAQ,CAAC,CAAC,KAAKW,IAAI;IAC9B,CAAC;IACDC,WAAWA,CAACC,KAAK,EAAE;MACf,IAAIb,QAAO,GAAI,IAAI,CAACC,MAAM,CAACC,IAAI,CAACC,KAAK,CAAC,GAAG;MACzC,OAAOH,QAAQ,CAACA,QAAQ,CAACI,MAAM,GAAC,CAAC,KAAKS,KAAK;IAC/C,CAAC;IACDC,aAAaA,CAAA,EAAG;MACZ,OAAO,IAAI,CAACtB,iBAAgB,IAAK,uBAAuB;IAC5D,CAAC;IACDuB,UAAUA,CAAA,EAAE;MACR,IA