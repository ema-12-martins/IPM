{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: 'LoginPage',\n  props: {\n    msg: String\n  },\n  data() {\n    return {\n      nome_mecanico: \"\",\n      pass_mecanico: \"\",\n      data: new Date().toLocaleDateString(),\n      hora: new Date().toLocaleTimeString().substring(0, 5),\n      num_posto: 4\n    };\n  },\n  methods: {\n    setNomeMecanico(event) {\n      this.nome_mecanico = event.target.value;\n    },\n    setPassMecanico(event) {\n      this.pass_mecanico = event.target.value;\n    },\n    validaMecanico() {\n      let nome = this.nome_mecanico.trim();\n      let pass = this.pass_mecanico.trim();\n      fetch(this.baseURL + '/mecanicos?nome_utilizador=' + nome).then(data => {\n        if (!response.ok) {\n          alert(\"O utilizador não se encontra na BD.\");\n        } else {\n          var mecanico = data;\n          if (mecanico.pass_mecanico == pass && mecanico.num_posto == this.num_posto) {\n            this.$router.push(\"/servicos/agendados\");\n          } else {\n            this.nome_mecanico = \"\";\n            this.pass_mecanico = \"\";\n            alert(\"Posto ou Pass incorreta\");\n          }\n        }\n      }).catch(() => {\n        alert(\"Não foi possivel conectar à BD.\");\n      });\n      if (nome === \"venancio\" && pass === \"1234\") {\n        this.$router.push(\"/servicos/agendados\");\n      } else {\n        this.nome_mecanico = \"\";\n        this.pass_mecanico = \"\";\n        alert(\"Dados incorretos\");\n      }\n    }\n  },\n  mounted() {\n    document.documentElement.classList.add('login-html-body');\n    document.body.classList.add('login-html-body');\n  },\n  beforeUnmount() {\n    document.documentElement.classList.remove('login-html-body');\n    document.body.classList.remove('login-html-body');\n  }\n};","map":{"version":3,"names":["name","props","msg","String","data","nome_mecanico","pass_mecanico","Date","toLocaleDateString","hora","toLocaleTimeString","substring","num_posto","methods","setNomeMecanico","event","target","value","setPassMecanico","validaMecanico","nome","trim","pass","fetch","baseURL","then","response","ok","alert","mecanico","$router","push","catch","mounted","document","documentElement","classList","add","body","beforeUnmount","remove"],"sources":["/home/emamartins12/GIT/IPM/ProjetoFinal/src/pages/Login.vue"],"sourcesContent":["<template>\n  <div class=\"app\" id=\"app\">\n      <!--Parte antes das caixas-->\n      <div class=\"container-parte_cima\">\n          <div class=\"container-login-bem_vindo_posto\">\n              <div class=\"login-bem_vindo\">Bem-vindo!</div>\n              <div class=\"login-posto\" >Encontra-se no posto {{ num_posto }}!</div>\n          </div>\n\n          <div class=\"container-data\">\n              <div class=\"login-data\">{{data}}</div>\n              <div class=\"login-horas\">{{hora}}</div>\n          </div>\n      </div>\n\n      <!--Caixas-->\n      <div class=\"container-grid\">\n          <div class=\"login-grid\">\n              <img src=\"../../images/person.png\" class='login-imagem' id=\"login-image1\" alt=\"Icon pessoa\">\n              <input type=\"text\" id=\"login-caixa1\" class=\"login-caixa\" name=\"texto\"  :value=\"nome_mecanico\" @input=\"setNomeMecanico\">\n\n              <img src=\"../../images/key.png\" class='login-imagem' id=\"login-image2\" alt=\"Icon pessoa\">\n              <input type=\"text\" id=\"login-caixa2\" class=\"login-caixa\" name=\"texto\" :value=\"pass_mecanico\" @input=\"setPassMecanico\">\n          </div>\n      </div>\n\n      <div class=\"container-login-pass\">\n          <!--Botao login-->\n          <button class=\"login-botao\" id=\"texto\" name=\"texto\" @click=\"validaMecanico\">Login</button>\n      </div>\n  </div>   \n</template>\n\n\n<script>\n\nexport default {\n  name: 'LoginPage',\n  props: {\n    msg: String,\n  },\n  data() {\n        return {\n            nome_mecanico: \"\",\n            pass_mecanico: \"\",\n            data: new Date().toLocaleDateString(),\n            hora: new Date().toLocaleTimeString().substring(0, 5),\n            num_posto: 4\n        }\n    },\n\n  methods: {\n        setNomeMecanico(event){\n          this.nome_mecanico=event.target.value\n        },\n        setPassMecanico(event){\n          this.pass_mecanico=event.target.value\n        },\n\n        validaMecanico() { \n          let nome = this.nome_mecanico.trim()\n          let pass = this.pass_mecanico.trim()\n\n          fetch(this.baseURL + '/mecanicos?nome_utilizador=' + nome) \n          .then(data => {\n            if(!response.ok){\n              alert(\"O utilizador não se encontra na BD.\")\n            }else{\n              var mecanico = data;\n              if(mecanico.pass_mecanico==pass && mecanico.num_posto==this.num_posto){\n                this.$router.push(\"/servicos/agendados\");\n              }else{\n                this.nome_mecanico=\"\"\n                this.pass_mecanico=\"\"\n                alert(\"Posto ou Pass incorreta\")\n                \n              }\n            }\n          })\n          .catch(()=>{\n            alert(\"Não foi possivel conectar à BD.\")\n          })\n\n\n\n          if (nome === \"venancio\" && pass === \"1234\") {\n            this.$router.push(\"/servicos/agendados\");\n          } else {\n            this.nome_mecanico=\"\"\n            this.pass_mecanico=\"\"\n            alert(\"Dados incorretos\")\n          }\n        }\n    },\n\n  mounted() {\n    document.documentElement.classList.add('login-html-body');\n    document.body.classList.add('login-html-body');\n  },\n  beforeUnmount() {\n    document.documentElement.classList.remove('login-html-body');\n    document.body.classList.remove('login-html-body');\n  }\n}\n\n</script>\n\n<style>\n.login-html-body{\n  height: auto;\n  padding: 0;\n  font-size: 0.8vw;\n  font-family: 'Open Sans', sans-serif;\n  background: linear-gradient(#DDEFFF, #002B53);\n  background-repeat: no-repeat;\n  background-attachment: fixed; \n}\n</style>\n\n\n<style scoped>\n.app {\n  /*Para centrar*/\n  top: 50%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n\n  position: absolute;\n  background-color: white;\n  color: black;\n  width: 50%;\n  height: auto;\n  border-radius: 30px; /* Arredondar as bordas em 5px */\n\n  display: flex;\n  flex-direction: column;\n  justify-content: space-around;\n  align-items: center;\n}\n\n/*-------------------------------PARTE DE CIMA---------------------------------------------------*/\n.login-bem_vindo{\n  text-align: center; /* Centralizar o texto horizontalmente */\n  font-size: 6rem;\n  font-weight: bold;\n}\n\n.login-posto{\n  text-align: center; \n  font-size: 2rem;\n}\n\n.login-data{\n  text-align: center; \n  font-size: 1.6rem;\n}\n\n.login-horas{\n  text-align: center;\n  font-size: 1.6rem;\n}\n\n/*--------------------------------CAIXAS E IMAGENS--------------------------------------------------*/\n.login-caixa {\n  display: flex;\n  width: 85%; \n  height: 40%; \n  padding: 1rem;\n  font-size: 1.5rem;\n  margin: 0 auto;\n  border: 1px solid #C4E6FF; /* Adicionar borda preta de 1px */\n  border-radius: 10px; /* Arredondar as bordas em 5px */\n  background-color: #C4E6FF;\n}\n\n.login-imagem{\n  margin-left: 3rem;\n  width: 60%; \n  height: 90%;\n  display: flex;\n}\n\n.login-grid{\n  display: grid;\n  grid-template-columns: 0.8fr 5fr;\n  grid-template-rows:auto auto;\n  grid-template-areas: \n      \"login-imagem1  login-caixa1\"\n      \"login-imagem2  login-caixa2\"\n      ;\n}\n\n#login-image1 {\n  grid-area: login-imagem1;\n}\n\n#login-caixa1 {\n  grid-area: login-caixa1;\n}\n\n#login-image2 {\n  grid-area: login-imagem2;\n}\n\n#login-caixa2 {\n  grid-area: login-caixa2;\n}\n\n/*-------------------------------------FUNDO---------------------------------------------------*/\n.login-botao{\n  display: block;\n  margin: 0 auto;\n  font-size: 2rem;\n  padding: 1rem 2rem;\n\n  border: 1px solid #C4E6FF; /* Adicionar borda preta de 1px */\n  border-radius: 10px; /* Arredondar as bordas em 5px */\n  background-color: #C4E6FF;\n  cursor: pointer; /*Passar o rato para uma mao quando estamos sobre o botao*/\n}\n\n.login-relembra_pass{\n  text-align: center; \n  font-size: 1.6rem;\n  cursor: pointer;\n}\n\n/*----------------------------Componentes-------------------------------------------------------*/\n\n.container-login-bem_vindo_posto{\n  display: flex;\n  flex-direction: column;\n  justify-content: space-between;\n  align-items: center;\n  height: 65%;\n}\n\n.container-data{\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n  height: 5%;\n  margin-top: 3rem;\n}\n\n.container-parte_cima{\n  margin-top: 3rem;\n  display: flex;\n  flex-direction: column;\n  justify-content: space-between;\n  align-items: center;\n}\n\n.container-login-pass{\n  margin-top: 2rem;\n  margin-bottom: 3rem;\n}\n\n.container-grid{\n  margin-top: 4rem;\n}\n</style>"],"mappings":";AAoCA,eAAe;EACbA,IAAI,EAAE,WAAW;EACjBC,KAAK,EAAE;IACLC,GAAG,EAAEC;EACP,CAAC;EACDC,IAAIA,CAAA,EAAG;IACD,OAAO;MACHC,aAAa,EAAE,EAAE;MACjBC,aAAa,EAAE,EAAE;MACjBF,IAAI,EAAE,IAAIG,IAAI,CAAC,CAAC,CAACC,kBAAkB,CAAC,CAAC;MACrCC,IAAI,EAAE,IAAIF,IAAI,CAAC,CAAC,CAACG,kBAAkB,CAAC,CAAC,CAACC,SAAS,CAAC,CAAC,EAAE,CAAC,CAAC;MACrDC,SAAS,EAAE;IACf;EACJ,CAAC;EAEHC,OAAO,EAAE;IACHC,eAAeA,CAACC,KAAK,EAAC;MACpB,IAAI,CAACV,aAAa,GAACU,KAAK,CAACC,MAAM,CAACC,KAAI;IACtC,CAAC;IACDC,eAAeA,CAACH,KAAK,EAAC;MACpB,IAAI,CAACT,aAAa,GAACS,KAAK,CAACC,MAAM,CAACC,KAAI;IACtC,CAAC;IAEDE,cAAcA,CAAA,EAAG;MACf,IAAIC,IAAG,GAAI,IAAI,CAACf,aAAa,CAACgB,IAAI,CAAC;MACnC,IAAIC,IAAG,GAAI,IAAI,CAAChB,aAAa,CAACe,IAAI,CAAC;MAEnCE,KAAK,CAAC,IAAI,CAACC,OAAM,GAAI,6BAA4B,GAAIJ,IAAI,EACxDK,IAAI,CAACrB,IAAG,IAAK;QACZ,IAAG,CAACsB,QAAQ,CAACC,EAAE,EAAC;UACdC,KAAK,CAAC,qCAAqC;QAC7C,CAAC,MAAI;UACH,IAAIC,QAAO,GAAIzB,IAAI;UACnB,IAAGyB,QAAQ,CAACvB,aAAa,IAAEgB,IAAG,IAAKO,QAAQ,CAACjB,SAAS,IAAE,IAAI,CAACA,SAAS,EAAC;YACpE,IAAI,CAACkB,OAAO,CAACC,IA