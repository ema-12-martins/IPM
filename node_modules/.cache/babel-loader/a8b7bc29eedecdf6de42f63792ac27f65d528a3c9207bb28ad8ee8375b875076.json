{\n        this.numeroAviso = 12;\n        this.mostrarAviso = true;\n\n        // Esconda o pop-up após 3 segundos\n        setTimeout(() => {\n          this.mostrarAviso = false;\n        }, 3000);\n      } else {\n        this.numeroAviso = 14;\n        this.mostrarAviso = true;\n        setTimeout(() => {\n          this.mostrarAviso = false;\n          this.$emit(\"guardardadosTabela\", this.selecionarIds);\n          this.$emit(\"fecharPop\");\n        }, 3000);\n      }\n    },\n    sair() {\n      if (this.selecionarIds.length === 0) {\n        this.$emit(\"fecharPop\");\n      } else {\n        this.motivoSair = true;\n      }\n    },\n    fecharPopAviso() {\n      this.motivoSair = false;\n    }\n  }\n};","map":{"version":3,"names":["PopUpsErro","MotivoSair","name","props","components","data","mostrarAviso","motivoSair","numeroAviso","pecas","id","parte","numero","selecionarIds","verde","computed","peloMenosUmSelecionado","umaSelecionada","some","selecionado","mounted","document","documentElement","classList","add","body","beforeUnmount","remove","methods","selecionarPeca","event","selectedId","target","value","checked","push","index","selectedIds","indexOf","splice","gravar","length","setTimeout","$emit","sair","fecharPopAviso"],"sources":["/home/emamartins12/GIT/IPM/ProjetoFinal/src/components/SugerirServicos.vue"],"sourcesContent":["<template>\n  <div id=\"app\" >\n    <div id=\"pag\">\n      <nav id=\"nav\">\n        <div class=\"nav-leftContainer\">\n          <div class=\"nav-titulo\" v-if=\"tituloPagina == false\">Pesquisar Peças</div>\n          <div class=\"nav-titulo\" v-if=\"tituloPagina == true\">Sugerir Serviços</div>\n        </div>\n\n        <div class=\"nav-rightContainer\">\n          <button class=\"nav-exit\" @click.prevent=\"sair\"></button>\n        </div>\n      </nav>\n\n      <main id=\"main\">\n        <div class=\"texto-inicial\">\n          Selecione os serviços que deveriam ser realizados:\n        </div>\n        <form>\n          <div class=\"conteudo\">\n            <div class=\"servico-form\" v-for=\"peca in pecas\" :key=\"peca.id\">\n              <label class=\"label\">{{ peca.parte }}</label>\n              <input\n                class=\"check-box\"\n                type=\"checkbox\"\n                name=\"servico1\"\n                :value=\"peca\"\n                v-model=\"selecionarIds\"\n                :change=\"selecionarPeca\"\n              />\n            </div>\n          </div>\n          <div class=\"popMostrarAvisoSelecionarVazio\" v-if=\"mostrarAviso\">\n            <PopUpsErro v-if=\"mostrarAviso\" :idPagina=\"numeroAviso\" />\n          </div>\n          <div class=\"popMostrarAvisoSelecionarVazio\" v-if=\"motivoSair\">\n            <MotivoSair @fecharPopUp=\"fecharPopAviso\">\n            </MotivoSair>\n          </div>\n          <div class=\"container-botao\">\n            <button v-if=\"\" class=\"botao-gravar\" @click.prevent=\"gravar\">Gravar</button>\n          </div>\n        </form>\n      </main>\n    </div>\n  </div>\n</template>\n\n<script>\nimport PopUpsErro from \"./PopUpsErro.vue\";\nimport MotivoSair from \"./MotivoSair.vue\";\nexport default {\n  name: \"SugerirServicos\",\n  props: [\"mostrarPop\", \"tituloPagina\"],\n  components: {\n    PopUpsErro,\n    MotivoSair,\n  },\n  data() {\n    return {\n      mostrarAviso: false,\n      motivoSair: false,\n      numeroAviso: 0,\n      pecas: [\n        { id: 1, parte: \"Discos de travão\", numero: 0 },\n        { id: 2, parte: \"Falta de Pneus\", numero: 0  },\n        { id: 3, parte: \"Falta de Oleo\", numero: 0 },\n      ],\n      selecionarIds: [],\n      verde:false,\n    };\n  },\n  computed: {\n    peloMenosUmSelecionado() {\n        // Verifica se pelo menos uma caixa de seleção está marcada\n        let umaSelecionada = this.selecionarIds.some(selecionado => selecionado)\n        if(umaSelecionada){\n          this.verde=true\n        }else{\n          this.verde=false\n        }\n    }\n  },\n  mounted() {\n    document.documentElement.classList.add(\"sugerirServicos-html-body\");\n    document.body.classList.add(\"sugerirServicos-html-body\");\n  },\n  beforeUnmount() {\n    document.documentElement.classList.remove(\"sugerirServicos-html-body\");\n    document.body.classList.remove(\"sugerirServicos-html-body\");\n  },\n  methods: {\n    selecionarPeca(event) {\n      const selectedId = event.target.value;\n      if (event.target.checked) {\n        // Add the id to the selectedIds array if checkbox is checked\n        this.selecionarIds.push(selectedId);\n      } else {\n        // Remove the id from the selectedIds array if checkbox is unchecked\n        const index = this.selectedIds.indexOf(selectedId);\n        if (index !== -1) {\n          this.selecionarIds.splice(index, 1);\n        }\n      }\n    },\n\n    gravar() {\n      // Verifique se selecionarIds está vazio\n      if (this.selecionarIds.length === 0) {\n        this.numeroAviso = 12;\n        this.mostrarAviso = true;\n\n        // Esconda o pop-up após 3 segundos\n        setTimeout(() => {\n          this.mostrarAviso = false;\n        }, 3000);\n      } else {\n        this.numeroAviso = 14;\n        this.mostrarAviso = true;\n        setTimeout(() => {\n          this.mostrarAviso = false;\n          this.$emit(\"guardardadosTabela\", this.selecionarIds);\n          this.$emit(\"fecharPop\");\n        }, 3000);\n      }\n    },\n    sair() {\n      if (this.selecionarIds.length === 0) {\n        this.$emit(\"fecharPop\");\n      } else {\n        this.motivoSair = true;\n      }\n    },\n    fecharPopAviso() {\n      this.motivoSair = false;\n    },\n  },\n};\n</script>\n\n<style>\n.sugerirServicos-html-body {\n  margin: 0;\n  padding: 0;\n  font-size: 0.8vw;\n}\n</style>\n\n<style scoped>\n.popMostrarAvisoSelecionarVazio {\n  position: fixed;\n  z-index: 1;\n  left: 0;\n  top: 0;\n  width: 100%;\n  height: 100%;\n  display: fixed;\n  justify-content: center;\n  align-items: center;\n  background-color: rgba(0, 0, 0, 0.5);\n}\n\n#app {\n  position: fixed;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n\n#pag {\n  border-radius: 1.9rem;\n  border: 0.2rem solid #000000;\n  background: #ffffff;\n  display: grid;\n  grid-template-areas:\n    \"nav\"\n    \"main\";\n  grid-template-rows: 1fr 13fr;\n  grid-template-columns: 1fr;\n  width: 70vw;\n}\n\n#nav {\n  background-color: #4a82aa;\n  grid-area: nav;\n  display: flex;\n  flex-direction: row;\n  justify-content: space-between;\n  border-top-left-radius: 1.7rem;\n  border-top-right-radius: 1.7rem;\n}\n\n#main {\n  grid-area: main;\n}\n\n.nav-titulo {\n  position: relative;\n  display: inline-block;\n  align-self: flex-start;\n  overflow-wrap: break-word;\n  display: inline-block;\n  word-wrap: break-word;\n  font-family: \"Inter\";\n  font-weight: 400;\n  font-size: 3rem;\n  color: #ffffff;\n}\n\n.nav-exit {\n  border: none;\n  background: none;\n  cursor: pointer;\n  background-image: url(\"../../images/x.svg\");\n  background-size: cover;\n  background-repeat: no-repeat;\n  width: 2.15rem;\n  height: 2.45rem;\n  margin-right: 0.5rem;\n}\n\n.nav-leftContainer {\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  margin-left: 1rem;\n}\n\n.nav-rightContainer {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  width: 3rem;\n  /* 12rem pelo primeiro e 7rem pelos restantes */\n  margin-right: 2rem;\n}\n\n.texto-inicial {\n  margin: 2rem 0 1rem 4rem;\n  overflow-wrap: break-word;\n  font-family: \"Inter\";\n  font-weight: 400;\n  font-size: 2.5rem;\n  color: #000000;\n}\n\n.conteudo {\n  background: #d9d9d9;\n  position: relative;\n  height: 32rem;\n  width: 80rem;\n  margin: 0 auto;\n  overflow: auto;\n}\n\n.servico-form {\n  background: #ffffff;\n  margin: 0.5rem 2rem 0.5rem 2rem;\n  display: inline-block;\n  width: 75rem;\n  height: 3rem;\n  border-radius: 1rem;\n}\n\n.check-box {\n  margin: 0.8rem 2rem 0 0;\n  float: right;\n  transform: scale(1.5);\n}\n\n.label {\n  margin-left: 2rem;\n  overflow-wrap: break-word;\n  font-family: \"Inter\";\n  font-weight: 400;\n  font-size: 2rem;\n  color: #000000;\n}\n\n.container-botao {\n  display: flex;\n  flex-direction: row;\n  justify-content: flex-end;\n  margin-right: 4.3rem;\n  margin-top: 2rem;\n  align-self: flex-end;\n}\n\n.botao-gravar {\n  background: #63aa6a;\n  position: relative;\n  padding: 0.5rem 0;\n  width: 12rem;\n  box-sizing: border-box;\n  overflow-wrap: break-word;\n  font-family: \"Inter\";\n  font-weight: 400;\n  font-size: 2.5rem;\n  color: #ffffff;\n}\n</style>\n"],"mappings":";AAiDA,OAAOA,UAAS,MAAO,kBAAkB;AACzC,OAAOC,UAAS,MAAO,kBAAkB;AACzC,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,KAAK,EAAE,CAAC,YAAY,EAAE,cAAc,CAAC;EACrCC,UAAU,EAAE;IACVJ,UAAU;IACVC;EACF,CAAC;EACDI,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,KAAK;MACnBC,UAAU,EAAE,KAAK;MACjBC,WAAW,EAAE,CAAC;MACdC,KAAK,EAAE,CACL;QAAEC,EAAE,EAAE,CAAC;QAAEC,KAAK,EAAE,kBAAkB;QAAEC,MAAM,EAAE;MAAE,CAAC,EAC/C;QAAEF,EAAE,EAAE,CAAC;QAAEC,KAAK,EAAE,gBAAgB;QAAEC,MAAM,EAAE;MAAG,CAAC,EAC9C;QAAEF,EAAE,EAAE,CAAC;QAAEC,KAAK,EAAE,eAAe;QAAEC,MAAM,EAAE;MAAE,CAAC,CAC7C;MACDC,aAAa,EAAE,EAAE;MACjBC,KAAK,EAAC;IACR,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,sBAAsBA,CAAA,EAAG;MACrB;MACA,IAAIC,cAAa,GAAI,IAAI,CAACJ,aAAa,CAACK,IAAI,CAACC,WAAU,IAAKA,WAAW;MACvE,IAAGF,cAAc,EAAC;QAChB,IAAI,CAACH,KAAK,GAAC,IAAG;MAChB,CAAC,MAAI;QACH,IAAI,CAACA,KAAK,GAAC,KAAI;MACjB;IACJ;EACF,CAAC;EACDM,OAAOA,CAAA,EAAG;IACRC,QAAQ,CAACC,eAAe,CAACC,SAAS,CAACC,GAAG,CAAC,2BAA2B,CAAC;IACnEH,QAAQ,CAACI,IAAI,CAACF,SAAS,CAACC,GAAG,CAAC,2BAA2B,CAAC;EAC1D,CAAC;EACDE,aAAaA,CAAA,EAAG;IACdL,QAAQ,CAACC,eAAe,CAACC,SAAS,CAACI,MAAM,CAAC,2BAA2B,CAAC;IACtEN,QAAQ,CAACI,IAAI,CAACF,SAAS,CAACI,MAAM,CAAC,2BAA2B,CAAC;EAC7D,CAAC;EACDC,OAAO,EAAE;IACPC,cAAcA,CAACC,KAAK,EAAE;MACpB,MAAMC,UAAS,GAAID,KAAK,CAACE,MAAM,CAACC,KAAK;MACrC,IAAIH,KAAK,CAACE,MAAM,CAACE,OAAO,EAAE;QACxB;QACA,IAAI,CAACrB,aAAa,CAACsB,IAAI,CAACJ,UAAU,CAAC;MACrC,OAAO;QACL;QACA,MAAMK,KAAI