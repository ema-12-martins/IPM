{"ast":null,"code":"import PopUpsErro from './PopUpsErro.vue';\nexport default {\n  name: 'MotivoSair',\n  emits: ['fecharPopUp', 'fecharPopUpTodo'],\n  components: {\n    PopUpsErro\n  },\n  data() {\n    return {\n      mostrarPopUp: false,\n      mostrarPai: true\n    };\n  },\n  methods: {\n    //Lógica para o botão não\n    voltarParaPaginaAnterior() {\n      this.$emit('fecharPopUp');\n    },\n    ativarPopUp() {\n      this.mostrarPopUp = true;\n      this.mostrarPai = false;\n      setTimeout(() => {\n        this.mostrarPopUp = false;\n        this.$emit('fecharPopUpTodo');\n      }, 3000);\n    }\n  },\n  mounted() {\n    document.documentElement.classList.add('motivoSair-html-body');\n    document.body.classList.add('motivoSair-html-body');\n  },\n  beforeUnmount() {\n    document.documentElement.classList.remove('motivoSair-html-body');\n    document.body.classList.remove('motivoSair-html-body');\n  }\n};","map":{"version":3,"names":["PopUpsErro","name","emits","components","data","mostrarPopUp","mostrarPai","methods","voltarParaPaginaAnterior","$emit","ativarPopUp","setTimeout","mounted","document","documentElement","classList","add","body","beforeUnmount","remove"],"sources":["/home/emamartins12/GIT/IPM/ProjetoFinal/src/components/MotivoSair.vue"],"sourcesContent":["<template>\n    <div id=\"container-Principal\" v-if=\"mostrarPai\">\n        <div id=\"PaginaMotivo\">\n            <nav id=\"nav\">  \n                <div class=\"nav-greetings\">\n                    Sair\n                </div>\n            </nav>\n    \n            <main id=\"main\">\n              <div class=\"container-FrasePrincipal\">\n                <div class=\"FrasePrincipal\">\n                    Tem a certeza que pretende sair?\n                </div>\n              </div>\n    \n              <div class=\"container-FraseSecundaria\">\n                <div class=\"FraseSecundaria\">\n                    As alterações efetuadas não serão guardadas...\n                </div>\n              </div>\n    \n                <div class=\"butoes\">\n                  <button class=\"butaoTexto\" @click=\"ativarPopUp()\">\n                    <span class=\"texto\">\n                      Sim\n                    </span>\n                    <div class=\"imagemCheckCross\">\n                      <img class=\"check\" src=\"@/../images/check.png\" />\n                    </div>\n                  </button>\n                    \n                  \n                  <button class=\"butaoTexto\" @click.prevent=\"voltarParaPaginaAnterior\">\n                    <span class=\"texto\">\n                      Não\n                    </span>\n                    <div class=\"imagemCheckCross\">\n                      <img class=\"cross\" src=\"@/../images/cross.png\" />\n                    </div>\n                  </button>\n                </div>\n            </main>\n        </div>  \n      </div>\n      <div class=\"popUpMotivoNaoGuardado\" v-if=\"mostrarPopUp\">\n        <PopUpsErro v-if=\"mostrarPopUp\" :idPagina=\"10\"/>\n        </div>\n</template>\n\n<script>\n\nimport PopUpsErro from './PopUpsErro.vue';\n\nexport default {\n    name: 'MotivoSair',\n    emits: ['fecharPopUp', 'fecharPopUpTodo'],\n    components:{\n        PopUpsErro\n    },\n\n    data() {\n        return {\n            mostrarPopUp: false,\n            mostrarPai: true,\n        }\n    },\n\n    methods:{\n        //Lógica para o botão não\n        voltarParaPaginaAnterior() {\n                this.$emit('fecharPopUp');\n        },\n        ativarPopUp(){\n            this.mostrarPopUp = true;\n            this.mostrarPai = false;\n            setTimeout(() => {\n            this.mostrarPopUp = false;\n            this.$emit('fecharPopUpTodo');\n            }, 3000);\n            \n        }\n    },    \n\n    mounted() {\n        document.documentElement.classList.add('motivoSair-html-body');\n        document.body.classList.add('motivoSair-html-body');\n    },\n    beforeUnmount() {\n        document.documentElement.classList.remove('motivoSair-html-body');\n        document.body.classList.remove('motivoSair-html-body');\n    }\n}\n</script>\n\n<style>\n.motivoSair-html-body{\n    margin: 0;\n    padding: 0;\n    font-size: 0.8vw;\n    height: 100%;\n}\n</style>\n\n<style scoped>\n#container-Principal {\n    height: 100vh;\n    display: flex;\n    justify-content: center; \n    align-items: center; \n}\n\n#PaginaMotivo {\n    border-radius: 1.9rem;\n    border: 0.2rem solid #000000;\n    background: #B1AAAA;\n    display: grid; \n    grid-template-areas:\n        \"nav\"\n        \"main\";\n    grid-template-rows: auto auto; \n    grid-template-columns: 1fr; \n    padding-bottom: 8.1rem;\n    width: 60.8%;\n}\n\n#nav {\n    grid-area: nav;\n    border-top-left-radius: 1.7rem;\n    border-top-right-radius: 1.7rem;\n    display: flex;\n    flex-direction: row;\n    justify-content: space-between;\n    background: #4A82AA;\n}\n\n.nav-greetings {\n    position: relative;\n    margin-left: 1.0rem;\n    display: inline-block;\n    align-self: flex-start;\n    overflow-wrap: break-word;\n    font-family: 'Inter';\n    font-weight: 400;\n    font-size: 3rem;\n    color: #FFFFFF;\n}\n\n#main {\n    grid-area: main;\n}\n\n.container-main{\n    display: flex;\n    flex-direction: column;\n    justify-content: space-between;\n}\n\n.container-FrasePrincipal{\n    margin-top: 5rem;\n    display: flex;\n    justify-content: center;\n}\n\n.FrasePrincipal{\n    overflow-wrap: break-word;\n    font-family: 'Inter';\n    font-weight: 400;\n    font-size: 4rem;\n    color: #000000;\n}\n\n.container-FraseSecundaria{\n    margin-top: 1.5rem;\n    margin-bottom: 4.5rem;\n    display: flex;\n    justify-content: center;\n}\n\n.FraseSecundaria{\n    overflow-wrap: break-word;\n    font-family: 'Inter';\n    font-weight: 400;\n    font-size: 2.8rem;\n    color: #000000;\n}\n\n.butoes{\n    display: flex;\n    flex-direction: row;\n    justify-content: space-between;\n    width: 53.6rem;\n    box-sizing: border-box;\n    margin: auto;\n}\n\n.butaoTexto{\n    background: #FFFFFF;\n    position: relative;\n    display: flex;\n    flex-direction: row;\n    padding: 0.9rem 0.95rem 0.9rem 0.95rem;\n    width: 41.5%;\n    box-sizing: border-box;\n    cursor: pointer;\n    border: 0.05rem solid;\n    overflow-wrap: break-word;\n    font-family: 'Inter';\n    font-weight: 400;\n    font-size: 4rem;\n    color: #000000;\n}\n\n.popUpMotivoNaoGuardado{\n    position: fixed;\n    z-index: 1;\n    left: 0;\n    top: 0;\n    width: 100%;\n    height: 100%;\n    display: fixed;\n    justify-content: center;\n    align-items: center;\n}\n\n.texto{\n    margin-right: 3rem;\n    margin-left: 3rem;\n}\n\n.imagemCheckCross{\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    width: 6rem;\n    height: 4.6rem;\n}\n\n.check{\n    width: 6rem;\n    height: 4.6rem;\n}\n\n.cross{\n    width: 5.5rem;\n    height: 4.6rem;\n}\n</style>"],"mappings":"AAoDA,OAAOA,UAAS,MAAO,kBAAkB;AAEzC,eAAe;EACXC,IAAI,EAAE,YAAY;EAClBC,KAAK,EAAE,CAAC,aAAa,EAAE,iBAAiB,CAAC;EACzCC,UAAU,EAAC;IACPH;EACJ,CAAC;EAEDI,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,YAAY,EAAE,KAAK;MACnBC,UAAU,EAAE;IAChB;EACJ,CAAC;EAEDC,OAAO,EAAC;IACJ;IACAC,wBAAwBA,CAAA,EAAG;MACnB,IAAI,CAACC,KAAK,CAAC,aAAa,CAAC;IACjC,CAAC;IACDC,WAAWA,CAAA,EAAE;MACT,IAAI,CAACL,YAAW,GAAI,IAAI;MACxB,IAAI,CAACC,UAAS,GAAI,KAAK;MACvBK,UAAU,CAAC,MAAM;QACjB,IAAI,CAACN,YAAW,GAAI,KAAK;QACzB,IAAI,CAACI,KAAK,CAAC,iBAAiB,CAAC;MAC7B,CAAC,EAAE,IAAI,CAAC;IAEZ;EACJ,CAAC;EAEDG,OAAOA,CAAA,EAAG;IACNC,QAAQ,CAACC,eAAe,CAACC,SAAS,CAACC,GAAG,CAAC,sBAAsB,CAAC;IAC9DH,QAAQ,CAACI,IAAI,CAACF,SAAS,CAACC,GAAG,CAAC,sBAAsB,CAAC;EACvD,CAAC;EACDE,aAAaA,CAAA,EAAG;IACZL,QAAQ,CAACC,eAAe,CAACC,SAAS,CAACI,MAAM,CAAC,sBAAsB,CAAC;IACjEN,QAAQ,CAACI,IAAI,CAACF,SAAS,CAACI,MAAM,CAAC,sBAAsB,CAAC;EAC1D;AACJ","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}