{"ast":null,"code":"import SairAplicacao from './SairAplicacao.vue';\nimport { LoginStore } from '../store/LoginStore.js';\nexport default {\n  name: 'navBar',\n  components: {\n    SairAplicacao\n  },\n  data() {\n    return {\n      mostrarPopUp: false\n    };\n  },\n  setup() {\n    const user = LoginStore();\n    return {\n      nome: user.getName\n    };\n  },\n  computed: {\n    dataAtual() {\n      const dataAtual = new Date();\n      const dia = String(dataAtual.getDate()).padStart(2, '0');\n      const mes = String(dataAtual.getMonth() + 1).padStart(2, '0');\n      const ano = dataAtual.getFullYear();\n      return `${dia}-${mes}-${ano}`;\n    },\n    horaAtual() {\n      const dataAtual = new Date();\n      const hora = String(dataAtual.getHours()).padStart(2, '0');\n      const minutos = String(dataAtual.getMinutes()).padStart(2, '0');\n      return `${hora}:${minutos}`;\n    }\n  },\n  methods: {\n    checkTipo(tipo) {\n      var listLink = this.$route.path.split('/');\n      return listLink[1] == tipo;\n    },\n    abrirPopUp() {\n      this.mostrarPopUp = true;\n    },\n    fecharPopUp() {\n      this.mostrarPopUp = false;\n    },\n    paginaAnterior() {\n      window.history.back();\n    }\n  }\n};","map":{"version":3,"names":["SairAplicacao","LoginStore","name","components","data","mostrarPopUp","setup","user","nome","getName","computed","dataAtual","Date","dia","String","getDate","padStart","mes","getMonth","ano","getFullYear","horaAtual","hora","getHours","minutos","getMinutes","methods","checkTipo","tipo","listLink","$route","path","split","abrirPopUp","fecharPopUp","paginaAnterior","window","history","back"],"sources":["/home/emamartins12/GIT/IPM/ProjetoFinal/src/components/Nav.vue"],"sourcesContent":["<template>\n    <div class=\"popUpSugerirServicos\" v-if=\"mostrarPopUp\">\n        <SairAplicacao @fecharPopUp=\"fecharPopUp()\"/>\n    </div>\n    <nav id=\"nav\">  \n        <div class=\"nav-leftContainer\">\n            <div class =\"nav-greetings\">\n                Bem-vindo, {{nome}}\n            </div>\n        </div>\n        \n        <div :class=\"{'nav-rightContainer2': checkTipo('servicos'), 'nav-rightContainer4': checkTipo('perfil'), 'nav-rightContainer3': checkTipo('servico')}\">\n            <button @click=\"abrirPopUp()\" v-if=\"checkTipo('perfil')\" class=\"nav-exit\"></button>\n            <button @click=\"paginaAnterior()\" v-if=\"!checkTipo('servicos')\" class=\"nav-return\"></button>\n            <div class=\"nav-date\">\n                {{ dataAtual }}\n                <br>\n                {{ horaAtual }}\n            </div>\n            <button class=\"nav-photo\"></button>\n        </div>\n    </nav>\n</template>\n\n\n<script>\nimport SairAplicacao from './SairAplicacao.vue';\nimport { LoginStore } from '../store/LoginStore.js'\n\nexport default {\n    name: 'navBar',\n\n    components: {\n        SairAplicacao\n    },\n\n    data() {\n        return {\n            mostrarPopUp: false\n        }\n    },\n\n    setup() {\n        const user = LoginStore()\n\n        return {\n            nome: user.getName\n        }\n    },\n\n    computed: {\n        dataAtual() {\n            const dataAtual = new Date();\n            const dia = String(dataAtual.getDate()).padStart(2, '0');\n            const mes = String(dataAtual.getMonth() + 1).padStart(2, '0');\n            const ano = dataAtual.getFullYear();\n            return `${dia}-${mes}-${ano}`;\n        },\n        horaAtual() {\n            const dataAtual = new Date();\n            const hora = String(dataAtual.getHours()).padStart(2, '0');\n            const minutos = String(dataAtual.getMinutes()).padStart(2, '0');\n            return `${hora}:${minutos}`;\n        }\n    },\n\n    methods: {\n        checkTipo(tipo) {\n            var listLink = this.$route.path.split('/')\n            return listLink[1] == tipo;\n        },\n        abrirPopUp(){\n            this.mostrarPopUp = true;\n        },\n        fecharPopUp(){\n            this.mostrarPopUp = false;\n        },\n        paginaAnterior(){\n            window.history.back();\n        }\n    }\n}\n</script>   \n\n\n<style scoped>\n\n#nav {\n    background-color: #4A82AA;\n    grid-area: nav;\n    display: flex;\n    flex-direction: row;\n    justify-content: space-between;\n}\n\n.nav-leftContainer{\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    margin-left: 1rem;\n}\n\n.nav-greetings {\n    display: inline-block;\n    word-wrap: break-word;\n    font-family: 'Inter';\n    font-weight: normal;\n    font-size: 3rem;\n    color: #FFFFFF;\n}\n\n.nav-rightContainer2 {\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    width: 19rem; /* 12rem pelo primeiro e 7rem pelos restantes */\n    margin-right: 1rem;\n}\n\n.nav-rightContainer3 {\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    width: 26rem; /* 12rem pelo primeiro e 7rem pelos restantes */\n    margin-right: 1rem;\n}\n\n.nav-rightContainer4 {\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    width: 33rem; /* 12rem pelo primeiro e 7rem pelos restantes */\n    margin-right: 1rem;\n}\n\n.nav-date {\n    display: inline-block;\n    text-align: center;\n    word-wrap: break-word;\n    font-family: 'Inter';\n    font-weight: normal;\n    font-size: 2rem;\n    color: #FFFFFF;\n}\n\n.nav-return {\n    border: none;\n    background: none;\n    cursor: pointer;\n    background-image: url('../../images/return.svg');\n    background-size: cover;\n    background-repeat: no-repeat;\n    width: 4.5rem;\n    height: 4rem;\n    margin-top: 0.75rem;\n    margin-bottom: 0.75rem;\n}  \n\n.nav-exit {\n    border: none;\n    background: none;\n    cursor: pointer;\n    background-image: url('../../images/exit.svg');\n    background-size: cover;\n    background-repeat: no-repeat;\n    width: 5.5rem;\n    height: 5.5rem;\n}\n\n.nav-photo {\n    border: none;\n    background: none;\n    cursor: pointer;\n    background-image: url('../../images/mecanico.png');\n    background-size: cover;\n    background-repeat: no-repeat;\n    width: 5.5rem;\n    height: 5.5rem;\n}\n\n</style>"],"mappings":"AA0BA,OAAOA,aAAY,MAAO,qBAAqB;AAC/C,SAASC,UAAS,QAAS,wBAAuB;AAElD,eAAe;EACXC,IAAI,EAAE,QAAQ;EAEdC,UAAU,EAAE;IACRH;EACJ,CAAC;EAEDI,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,YAAY,EAAE;IAClB;EACJ,CAAC;EAEDC,KAAKA,CAAA,EAAG;IACJ,MAAMC,IAAG,GAAIN,UAAU,CAAC;IAExB,OAAO;MACHO,IAAI,EAAED,IAAI,CAACE;IACf;EACJ,CAAC;EAEDC,QAAQ,EAAE;IACNC,SAASA,CAAA,EAAG;MACR,MAAMA,SAAQ,GAAI,IAAIC,IAAI,CAAC,CAAC;MAC5B,MAAMC,GAAE,GAAIC,MAAM,CAACH,SAAS,CAACI,OAAO,CAAC,CAAC,CAAC,CAACC,QAAQ,CAAC,CAAC,EAAE,GAAG,CAAC;MACxD,MAAMC,GAAE,GAAIH,MAAM,CAACH,SAAS,CAACO,QAAQ,CAAC,IAAI,CAAC,CAAC,CAACF,QAAQ,CAAC,CAAC,EAAE,GAAG,CAAC;MAC7D,MAAMG,GAAE,GAAIR,SAAS,CAACS,WAAW,CAAC,CAAC;MACnC,OAAQ,GAAEP,GAAI,IAAGI,GAAI,IAAGE,GAAI,EAAC;IACjC,CAAC;IACDE,SAASA,CAAA,EAAG;MACR,MAAMV,SAAQ,GAAI,IAAIC,IAAI,CAAC,CAAC;MAC5B,MAAMU,IAAG,GAAIR,MAAM,CAACH,SAAS,CAACY,QAAQ,CAAC,CAAC,CAAC,CAACP,QAAQ,CAAC,CAAC,EAAE,GAAG,CAAC;MAC1D,MAAMQ,OAAM,GAAIV,MAAM,CAACH,SAAS,CAACc,UAAU,CAAC,CAAC,CAAC,CAACT,QAAQ,CAAC,CAAC,EAAE,GAAG,CAAC;MAC/D,OAAQ,GAAEM,IAAK,IAAGE,OAAQ,EAAC;IAC/B;EACJ,CAAC;EAEDE,OAAO,EAAE;IACLC,SAASA,CAACC,IAAI,EAAE;MACZ,IAAIC,QAAO,GAAI,IAAI,CAACC,MAAM,CAACC,IAAI,CAACC,KAAK,CAAC,GAAG;MACzC,OAAOH,QAAQ,CAAC,CAAC,KAAKD,IAAI;IAC9B,CAAC;IACDK,UAAUA,CAAA,EAAE;MACR,IAAI,CAAC5B,YAAW,GAAI,IAAI;IAC5B,CAAC;IACD6B,WAAWA,CAAA,EAAE;MACT,IAAI,CAAC7B,YAAW,GAAI,KAAK;IAC7B,CAAC;IACD8B,cAAcA,CAAA,EAAE;MACZC,MAAM,CAACC,OAAO,CAACC,IAAI,CAAC,CAAC;IACzB;EACJ;AACJ","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}